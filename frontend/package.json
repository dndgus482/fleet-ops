    "axios": "^1.9.0",
    "pinia": "^3.0.2",
    "qs": "^6.14.0",
    "sockjs-client": "^1.6.1",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1",
    "zod": "^3.25.48"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.29",
    "@types/qs": "^6.14.0",
    "@types/sockjs-client": "^1.5.4",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.0",
